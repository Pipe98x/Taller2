            <string>/// movimiento
key_right = keyboard_check(vk_right);
key_left = keyboard_check(vk_left);
key_space = keyboard_check(vk_space);

///operaciones
mov = key_right - key_left;
hsp = mov * movespeed;

/// colision horizontal

if(place_meeting(x+hsp, y , obj_muro)){
    while(!place_meeting(x+sign(hsp), y , obj_muro or obj_puerta)) {
    x +=sign(hsp); }
    hsp = 0
}

/// colision vertical

if(place_meeting(x, y+vsp , obj_muro or obj_puerta)){
    while(!place_meeting(x, y + sign(vsp) , obj_muro or obj_puerta)) {
    y +=sign(vsp); }
    vsp = 0
}
    
x+= hsp;

if(vsp &lt; 10) {vsp += grave;}

if(place_meeting(x,y+1,obj_muro or obj_puerta)) {
    vsp = key_space * -jumpspeed;
}

y += vsp;

///animacion

if (mov!=0) image_xscale = mov;
if (place_meeting(x,y+1,obj_muro or obj_puerta))
{
    if (mov!=0) sprite_index = spr_personaje_corre; else sprite_index = spr_personaje_pie;
}
else
{
    if (vsp &lt; 0) sprite_index = spr_personaje_salta; else sprite_index = spr_personaje_cae;
}
