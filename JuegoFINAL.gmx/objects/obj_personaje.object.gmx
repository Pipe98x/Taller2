        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Movimiento y animaci√≥n
global.posicion= x;
if (global.iniciar=false){
    distancia=0
    }
    else{
    distancia=string_format(distance_to_object(obj_linea)/64,2,1);
    show_debug_message(distancia)
    }
/// movimiento
key_right = keyboard_check(vk_right);
key_left = keyboard_check(vk_left);
key_space = keyboard_check(vk_space);
key_down = keyboard_check(vk_down);

///operaciones
mov = key_right - key_left;
hsp = mov * movespeed;

/// colision horizontal

if(place_meeting(x+hsp, y , obj_muro)){
    while(!place_meeting(x+sign(hsp), y , obj_muro)) {
    x +=sign(hsp); }
    hsp = 0
}

/// colision vertical

if(place_meeting(x, y+vsp , obj_muro)){
    while(!place_meeting(x, y + sign(vsp) , obj_muro)) {
    y +=sign(vsp); }
    vsp = 0
}
    
x+= hsp;

if(vsp &lt; 10) {vsp += grave;}

if(place_meeting(x,y+1,obj_muro)) {
    vsp = key_space * -jumpspeed;
}

y += vsp;
///animacion
if key_down {sprite_index=spr_desliza;}
if (mov!=0) image_xscale = mov;
if (place_meeting(x,y+1,obj_muro))
{
    if (mov!=0 &amp;&amp; !key_down &amp;&amp; !place_meeting(x,y-40,obj_muro)) sprite_index = spr_personaje_corre; else { if(mov!=0 &amp;&amp; key_down) sprite_index = spr_desliza; else if (mov=0 &amp;&amp; !place_meeting(x,y-40,obj_muro)) sprite_index= spr_personaje_pie}
}
else
{
    if (vsp &lt; 0) sprite_index = spr_personaje_salta; else sprite_index = spr_personaje_cae;
}

//tutotial volver al menu

if key_down &amp;&amp; key_space &amp;&amp; room=tutorial{room_goto(Menu)}
</string>
